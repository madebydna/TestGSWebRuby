{
  "name": "GSWeb",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "5.10.0",
    "npm": "3.5.0"
  },
  "scripts": {
    "postinstall": "cd client && npm install",
    "rails-server": "echo 'visit http://localhost:3000' && foreman start -f Procfile.dev",
    "test": "cd client && npm test",
    "test:watch": "cd client && npm run test -- --watch",
    "build:development": "cd client && npm run build:development",
    "build:test": "cd client && npm run build:test",
    "build:production": "cd client && npm run build:production",
    "analyze": "cd client && npm run analyze",
    "rspec": "cd client && npm run rspec"
  },
  "dependencies": {
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^2.5.1"
  }
}
