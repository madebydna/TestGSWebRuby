<% #Header Section %>
<div class='section-header'>
  <h2 class="lg-roboto"><%= t(".#{@t_scope}.title") %></h2>
  <div class='header-subtext open-sans_sb'><%= t(".#{@t_scope}.subtitle") %></div>
</div>
<div class='row mtl'>
  <% #Ratings1 %>
  <div class='col-xs-12 col-sm-6 pal'>
    <div class='clearfix mrm pull-left'>
      <div class="gs-rating-white-sm rating-icon-margin above_average">10</div>
      <div class="gs-rating-white-sm rating-icon-margin average">5</div>
      <div class="gs-rating-white-sm rating-icon-margin below_average">1</div>
    </div>
    <span class='ratings-sub-title'><%= t(".#{@t_scope}.paragraph_title_1") %></span>
    <div><%= t(".#{@t_scope}.paragraph_description_1_html") %></div>
  </div>
  <% #Ratings2 %>
  <div class='col-xs-12 col-sm-6 pal'>
    <div class='clearfix mrm pull-left'>
      <div class="gs-rating-smallish rating-icon-margin above_average">10</div>
      <div class="gs-rating-smallish rating-icon-margin average">5</div>
      <div class="gs-rating-smallish rating-icon-margin below_average">1</div>
    </div>
    <span class='ratings-sub-title'><%= t(".#{@t_scope}.paragraph_title_2") %></span>
    <div><%= t(".#{@t_scope}.paragraph_description_2") %></div>
  </div>
</div>
