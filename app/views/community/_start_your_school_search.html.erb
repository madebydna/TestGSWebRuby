<% #Header Section %>
<div class='section-header'>
  <h2 class="lg-roboto"><%= t(".#{@t_scope}.title") %></h2>
  <div class='header-subtext open-sans_sb'><%= t(".#{@t_scope}.subtitle") %></div>
</div>

<div class="container-fluid">
  <div class='row'>
    <% #Ratings1 %>
    <div class='col-xs-12 col-sm-6 pal'>
        <%= render partial: 'community/school_rating_sample' %>
        <span class='ratings-sub-title'><%= t(".#{@t_scope}.paragraph_title_1") %></span>
        <div><%= t(".#{@t_scope}.paragraph_description_1_html") %></div>
    </div>

    <% #Ratings2 %>
    <div class='col-xs-12 col-sm-6 pal'>
        <%= render partial: 'community/comparison_barchart_sample' %>
        <span class='ratings-sub-title'><%= t(".#{@t_scope}.paragraph_title_2") %></span>
        <div><%= t(".#{@t_scope}.paragraph_description_2") %></div>
    </div>
  </div>
</div>
