<% category_placement %>

<% if category_placement.present? %>

    <% begin %>
        <%= render category_placement.partial,
          category_placement: category_placement,
          data: category_placement_data(category_placement),
          category: category_placement.category
        %>
    <% rescue => e %>
        <%= log_view_error "Something went wrong when trying to render category placement #{category_placement.title}", e %>
        <% raise e if params[:debug] %>
    <% end %>

<% end %>

