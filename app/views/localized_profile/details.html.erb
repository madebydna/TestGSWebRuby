<%= render 'nav/topnav' %>
<%= render 'localized_profile/header', school: @school %>
<%= render 'localized_profile/profile-nav', school: @school %>

<div class="row limit_width_1200">
    <div class="col-xs-12 col-sm-12 col-md-3 col-lg-3">
        <div  data-spy="affix" class="spy-nav">
            <ul class="nav nav-list nav-stacked">
                <% @page_config.placements.each do |placement| %>
                    <li>
                        <a href="#<%= category_placement_anchor placement %>">
                            <%= category_placement_title placement %></a>
                    </li>
                <% end %>
            </ul>
        </div>
    </div>
    <div class="col-xs-12 col-sm-12 col-md-9 col-lg-9" role="main">
        <div class="row">
            <%= render partial: 'position', collection: (1..50).to_a, as: :position_number %>
        </div>
    </div>
</div>