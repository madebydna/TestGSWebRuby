<%= render 'nav/topnav' %>
<%= render 'localized_profile/header', school: @school %>
<%= render 'localized_profile/profile-nav', school: @school %>



<!--<%= @school.test_scores %>-->

<% @school.test_scores.each do |test,value| %>
    <div class="contents">
        <div class="main">
            <div class="row">
                <div class="col-xs-12 col-sm-4 col-md-3 col-lg-3">
                    <div>
                        <%= test %><br/>
                        <% value.each do |grade,value| %>
                            <br/>GRADE <%= grade %><br/><br/>
                            <% value.each do |levelcode,value| %>
                                <% value.each do |subject,value| %>
                                    <%= subject %><br/>
                                    <% value.each do |year,value| %>
                                        <%= year %>
                                        score: <%= value["score"] %>
                                        number tested: <%= value["number_tested"] %><br/>
                                    <% end %>
                                <% end %>
                            <% end %>
                        <% end %>
                    </div>
                </div>
            </div>
        </div>
    </div>
<% end %>
<%= render 'nav/footer' %>