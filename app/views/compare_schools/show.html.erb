
<% content_for :page_javascripts do %>
<%= webpack_javascript_include_tag 'react-redux', defer: true %>
<%= webpack_javascript_include_tag 'compare', defer: true %>
<% end %>
<% content_for :page_stylesheets do %>
<%= stylesheet_link_tag "post_load", media: "all", "data-turbolinks-track" => true %>
<%= stylesheet_link_tag "compare", async: true %>
<% end %>

<div id="compare-schools">
  <%= react_component("Compare", props: {
    schools: @schools,
    distance: @radius
  }) %>
</div>
