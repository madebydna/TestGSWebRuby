<% content_for :head do %>
    <link rel="dns-prefetch" href="//maps.googleapis.com">
<% end %>
<% content_for :page_javascripts do %>
  <%= webpack_javascript_include_tag 'react-redux', defer: true %>
  <%= webpack_javascript_include_tag 'district-boundaries', defer: true %>
<% end %>
<% content_for :page_stylesheets do %>
  <%= stylesheet_link_tag "district-boundaries", async: true %>
<% end %>

<div id="district-boundaries-page">
  <div class="header">
    <h1><%= t('.heading.') %> <%= render 'school_profiles/info_circle', content: t('.subheading_html'), element_type: '' %></h1>
    <div class="legend-cta">
      <%= react_component('DistrictBoundariesLegend') %>
    </div>
  </div>
  <%= react_component('ConnectedDistrictBoundaries') %>
</div>
