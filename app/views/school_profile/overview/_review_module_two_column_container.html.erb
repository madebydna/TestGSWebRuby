<% if @school_reviews.number_of_reviews_with_comments > 0 %>
  <div id="old-reviews-section" class="row">
      <div style="zindex: 1;">
          <%= render "shared/section_header",
                     category_placement:category_placement,
                     link_text: @school_reviews.see_all_reviews_phrase,
                     link_to:'reviews' %>
  
        <div class="js_reviewsList">
            <%= render 'school_profile/overview/review_module_two_column', reviews: @school_reviews.having_comments %>
        </div>
    </div>
  </div>
<% end %>
