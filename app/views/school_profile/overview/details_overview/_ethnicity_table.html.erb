<div class="table-condensed">
  <table class="table table-striped-inverted gray-box-border mtm mbm" style="">
    <tbody>
      <% Hash[data.take(5)].each do |k,v| %>
        <tr class="font-size-xs">
          <td style="line-height:.5"> <%= k %></td>
          <td style="line-height:.5" > <%= v.round(0) %>%</td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
<%= link_to t('.more'), school_details_url(@school, anchor:"Dig_deeper") %>
