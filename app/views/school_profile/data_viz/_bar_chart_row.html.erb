<div class="col-xs-12 col-md-6 bar-chart-rows">
  <% #Row header %>
  <div class='header'>
    <div class='row-text'><%= row.bar_text %></div>
    <div class='row-text' style='color:<%= row.color %>' ><%= row.bar_color_value %> <%= row.bar_value_text %></div>
  </div>

  <% #Row bar %>
  <div class="bar-row">
    <div class="pr clearfix">
      <div class="color-row" style="width:<%= row.bar_color_value %>; background-color:<%= row.color %>;"></div>
      <div class="grey-row" style="width:<%= row.bar_grey_value %>;"></div>
      <div class="i-data-viz-state-avg state-avg-marker" style="left:<%= row.cursor_value %>"></div>
    </div>
  </div>
  
  <% #Subtext %>
  <div class='subtext'>
    <div><%= row.sub_text %></div>
  </div>
</div>
