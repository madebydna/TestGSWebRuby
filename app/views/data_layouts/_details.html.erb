<%= render layout:'col', locals: { css_class:'', category_placement: category_placement } do %>
        <div class="plm">
            <hr class="gold-bar" />
            <h3 class="pll">Details</h3>
        </div>
            <div class="pal">
            <div class="row">
                <% if @school.includes_level_code? (['e','m','h']) %>
                    <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3"><%= render "shared/icon_content", data_local: data[:art] %></div>
                    <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3"><%= render "shared/icon_content", data_local: data[:sport] %></div>
                    <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3"><%= render "shared/icon_content", data_local: data[:club] %></div>
                    <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3"><%= render "shared/icon_content", data_local: data[:lang] %></div>
                <% end %>
                <% if @school.includes_preschool? %>
                    <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3"><%= render "shared/icon_content", data_local: data[:sched] %></div>
                    <div class="col-xs-12 col-sm-6 col-md-3 col-lg-3"><%= render "shared/icon_content", data_local: data[:commu] %></div>
                <% end %>
            </div>
        </div>
<% end %>