<% data # A +Hash+ that contains a preschool rating +Hash+ if available %>
<%= debug data if params[:debug] %>
<% rating_hash = data["preschool_rating"] %>

<% if rating_hash.present? && rating_hash["overall_rating"].present? %>
  <div class="row mbl pam">
    <% rating_type = '' %>
    <% rating_star = rating_hash["overall_rating"] %>
    <% if rating_star == 0 %>
      <% rating_type = '-outline' %>
      <% rating_star = '1' %>
    <% end %>
    <div class="col-xs-12 col-sm-3 col-md-2 col-lg-2">
      <span class="iconx24-stars_detroit i-24-blue<%= rating_type %>-star i-24-star-<%= rating_star %>"></span>
    </div>
    <div class="col-xs-12 col-sm-9 col-md-10 col-lg-10">
      <h5><%= rating_hash["label"] %></h5>
      <br/><%= rating_hash["description"] %>
    </div>
  </div>
<% end %>


