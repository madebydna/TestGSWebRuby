<div id="piechart">

</div>

<% ethnicity_array = data.to_piechart :ethnicity, :school_value %>
<% piechart_options = {
        width: 350,
        height: 350,
        fontSize: '13',
        title: category.name
    }.stringify_keys;
%>

<% content_for :head do %>
    <script type="text/javascript">
        GS.visualchart.drawPieChart(
            <%= raw ethnicity_array.to_json %>,
            'piechart',
            null,
            <%= raw config.config.merge(piechart_options).to_json %>
        );
    </script>
<% end %>