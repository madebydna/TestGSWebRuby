<div class="col-xs-12 col-sm-<%= size %> col-md-<%= size %>">
    <div class="contents">
        <div class="main">
            <a name="<%= category_placement.position.to_s + category.code_name %>"></a>
            <p style="padding-top: 50px; margin-top: -50px;"><!-- do not collapse --></p>
<div id="piechart">

</div>

<% ethnicity_array = data.to_piechart :ethnicity, :school_value %>
<% piechart_options = {
        width: 350,
        height: 350,
        fontSize: '13',
        title: category.name
    }.stringify_keys;
%>

<% content_for :head do %>
    <script type="text/javascript">
        GS.visualchart.drawPieChart(
            <%= raw ethnicity_array.to_json %>,
            'piechart',
            null,
            <%= raw config.config.merge(piechart_options).to_json %>
        );
    </script>
<% end %>
</div>
</div>
</div>