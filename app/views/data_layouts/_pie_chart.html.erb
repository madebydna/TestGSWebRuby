<% data_config = category_placement.table_config %>
<%= render layout:'col', locals: { css_class:'', category_placement: category_placement } do  %>

    <p style="padding-top: 50px; margin-top: -50px;"><!-- do not collapse --></p>
    <div id="piechart" class="ma">

    </div>

    <% ethnicity_array = data.to_piechart :ethnicity, :school_value %>
    <% piechart_options = {
            fontSize: '13',
            title: category.name
        }.stringify_keys;
    %>

    <% content_for :head do %>
        <script type="text/javascript">
            GS.visualchart.drawPieChart(
                <%= raw ethnicity_array.to_json %>,
                'piechart',
                null,
                <%= raw data_config.config.merge(piechart_options).to_json %>,
                'ethnicity'
            );
        </script>
    <% end %>

<% end %>
