<% if data.present? %>
  <div class='mbm'><%= t('.score_range') %></div>
  <div class='row mbl'>
    <% [*data].each do |d| %>
      <div class="col-xs-12 col-md-6 col-lg-4 mbm">
        <div class="gs-rating-white-sm sub-rating-icon <%= d.performance_level %>">
          <%= d.value %>
        </div>
        <span class="sub-rating-text"><%= d.label %></span>
        <%= render 'school_profile/data_viz/tooltip_activator', text: d.label %>
      </div>
    <% end %>
  </div>
<% end %>
