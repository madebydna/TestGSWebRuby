<% data.each_with_index do |(test,value),index| %>
    <div class="contents">
        <div class="main">
            <%= render layout:'col', locals: { css_class:'clearfix pam', category_placement: category_placement } do %>
                    <%= value[:test_label] %><br/><br/>
                    <div class="btn-group js_grades_div js_bootstrapExtButtonSelect">
                        <% value[:grades].each do |grade,value| %>
                            <button id="js_<%= test %>_<%= grade.get_value %>" class="btn btn-default js_test_scores_grades">GRADE <%= grade.get_value %></button>
                        <% end %>
                    </div>

                    <% value[:grades].each do |grade,value| %>
                        <div id="js_<%= test %>_<%= grade.get_value %>_scores" class="js_<%= test %>_grades">

                            <% value.each do |levelcode,value| %>
                                <% value.each do |subject,value| %>
                                    <br/><%= subject.gs_capitalize_first %>
                                    <!--TODO change this to test id-->
                                    <% bar_chart_div_id = "js_bar_chart_div_#{test.to_s.gsub(/\s/,'')}_#{grade.to_s}_#{subject.to_s.gsub(/\s/,'')}" %>


                                    <div id="<%= bar_chart_div_id %>"><!--Div for bar chart--></div>
                                    <% content_for :head do %>
                                        <script type="text/javascript">
                                            GS.visualchart.drawBarChart(<%= raw (to_bar_chart_array value).to_s %>,<%= raw '"'+bar_chart_div_id+'"' %>);
                                        </script>
                                    <% end %>
                                <% end %>
                            <% end %>
                        </div>
                    <% end %>

            <% end %>
        </div>
    </div>
<% end %>
