<% data # A +Hash+ containing data for this table to display %>
<%= debug data if params[:debug] %>

<% if data.present? && data.first[:value].present? && Array.wrap(data.first[:value]).first.present? %>
  <%= render layout:'col', locals: { css_class:'phs', category_placement: category_placement } do %>
    <%= Array.wrap(data.first[:value]).first %>
  <% end %>
<% end %>
