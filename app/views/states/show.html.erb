<% content_for :javascript_includes do %>
  <script>
    $(document).ready(function() {
      GS.viewMoreCollapseInit();
      GS.track.setOmnitureData();
    });
  </script>
<% end %>

<%= render 'nav/topnav', hub_params: @hub_params %>

<%= render 'shared/search_hero', collection_nickname: @collection_nickname,
                          state: @state,
                          browse_links: @browse_links,
                          hero_image: @hero_image,
                          hero_image_mobile: @hero_image_mobile,
                          sponsor: @sponsor,
                          state_page: true %>

<div class="limit-width-1200 mvl">
  <%= render 'shared/choose_a_school', choose_school: @choose_school %>
  <%= render 'content_module', content_modules: @content_modules %>
  <%= render 'shared/review_global'%>
  <%= render 'featured_articles', articles: @articles, state: @state %>
</div>

<%= render 'partners', partners: @partners %>
<% if @show_ads %>
    <% ad_hash_desktop_footer={visible_for:'visible-lg visible-md visible-sm',ad_slot_id:'Ad_Id_Responsive_State_Footer',ad_size:'[728,90]',ad_width:'728',ad_target:'Responsive_State_Home_Footer_728x90'}%>
    <% ad_hash_mobile_footer={visible_for:'visible-xs',ad_slot_id:'Ad_Id_Responsive_State_Mobile_Footer',ad_size:'[320,50]',ad_width:'320',ad_target:'Responsive_State_Home_Mobile_Footer_320x50'}%>

    <div class="mvl">
      <%= render 'shared/ad_slot', ad_data: ad_hash_desktop_footer %>
      <%= render 'shared/ad_slot', ad_data: ad_hash_mobile_footer %>
    </div>

<% end %>
<%= render 'shared/state_footer', state: @state, cities: @cities %>
