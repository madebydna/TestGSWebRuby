    <% school_reviews_pagination.each do |review| %>
        <div class="contents">
            <div class="main">
                <div class="row">
                    <div class="col-xs-12 col-sm-4 col-md-3 col-lg-3">
                        <div>
                            <%= draw_stars_16 review[:quality].to_i %>
                        </div>
                        <br />
                        posted on: <%= review[:posted] %><br />
                        posted by: <%= review[:who] %>
                    </div>

                    <div class="col-xs-12 col-sm-8 col-md-9 col-lg-9">
                         <%= review[:comments] %>
                     </div>

                </div>
            </div>
        </div>
    <% end %>

