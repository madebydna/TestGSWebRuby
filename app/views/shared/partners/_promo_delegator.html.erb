<% force_show ||= false %>
<% collections.each do |collection| %>
  <% if (promo = collection.config[:promo]) %>
    <% if force_show || promo[:profile_modules].include?(partial) %>
      <%= render partial: "shared/partners/#{promo[:type]}",
                locals: {
                  promo: promo
                }
      %>
    <% end %>
  <% end %>
<% end %>
