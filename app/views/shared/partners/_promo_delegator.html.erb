<% force_show ||= false %>
<% options ||= {} %>
<% promo_type ||= nil %>
<% promos = promos.select { |p| p[:type] == promo_type } if promo_type.present? %>
<% promos.each do |promo| %>
  <% if promo[:profile_modules].include?(partial) || force_show %>
    <%= render partial: "shared/partners/#{promo[:type]}",
              locals: {
                promo: promo,
                options: options
              }
    %>
  <% end %>
<% end %>
