<div id="js-assigned-school-<%=level.downcase%>" class="dn pvm js-schoolSearchResultCompareErrorMessage">
    <div class="visible-xs visible-sm pvs plm font-size-small inverted-text bg-neutral uc brs"
         style="width: 140px; border-bottom-right-radius: 0;"><%= t('.label') %></div>
    <div class="clearfix search-result-border emphasis-border prm bg-white">
        <div class="">
            <div class="row">
                <div class="col-xs-12 clearfix">
                    <div class="fl hidden-xs hidden-sm" style="width:140px;">
                        <div class="pvs plm font-size-small inverted-text bg-neutral uc"><%= t('.label') %></div>
                    </div>
                    <div class="mll fl ptm hidden-xs hidden-sm">
                        <div class="font-size-small gray-darker">
                          <%= t('.double_check_with_district_or_school', level: level) %><span class="brand-primary">*</span>
                        </div>
                        <hr class="mvn"/>
                    </div>
                    <div class="hidden-md hidden-lg col-xs-12">
                      <div class="font-size-small gray-darker">
                        <%= t('.double_check_with_district_or_school', level: level) %><span class="brand-primary">*</span>
                      </div>
                      <hr class="mvn"/>
                    </div>
                </div>
                <div class="col-xs-7 col-sm-5 col-md-7 col-lg-6 clearfix school-info">
                    <div class="fl hidden-xs hidden-sm js-photo">
                        <img class="thumbnail-border" src="<%= image_path('search/no-school-photo.png')%>" alt="<%= t('.no_school_photo_alt_text') %>"/>
                    </div>
                    <div class="fl pll">
                        <div class="ptm notranslate">
                            <div><a class="js-name open-sans_sb mbs font-size-medium" style="line-height: 1.1em"></a></div>
                            <div class="hidden-xs font-size-small js-address"></div>
                            <div class="clearfix visible-xs font-size-small">
                                <div class="prs fl js-type"></div>
                                <div class="fl js-grade-range"></div>
                            </div>
                        </div>
                        <div class="font-size-small mvm clearfix ptm hidden-xs">
                            <div class="fl">
                                <span class="prm vam js-parent-rating-stars"><%= draw_stars_16 1 %></span>
                                <a class="font-size-small js-review-count"></a>
                                <em class="js-no-reviews"><%= t('.no_community_reviews') %></em>
                            </div>
                            <div class="phm fl">|</div>
                            <div class="prs fl js-type"></div>
                            <div class="fl js-grade-range"></div>
                            <div class="visible-xs phm fl">|</div>
                            <div class="visible-xs fl js-distance"></div>
                        </div>
                    </div>
                </div>
                <div class="col-xs-5 col-sm-4 col-md-3 col-lg-4 icons">
                    <div class="ptl hidden-xs"></div>
                    <div class="row">
                        <!-- Distance -->
                        <div class="hidden-xs col-sm-4 mtl mbm font-size-xs">
                            <div class="iconx24-icons i-24-new-grey_map_pin ma db vam"></div>
                            <span class="tac db js-distance"></span>
                        </div>
                        <!-- GS Rating -->
                        <div class="col-xs-6 col-sm-4 mvl">
                            <a class="js-gs-rating-link">
                              <div class="ma tac"><%= render 'performance/ten_point_gs_rating', value: 5, size: 'xtra-small', css_class: 'js-gs-rating-icon' %></div>
                                <div class="pts"></div>
                                <span class="tac font-size-xs db link-darkgray" style="line-height: 1.3em">
                                  <%= t('.greatschools_rating_html') %>
                                </span>
                            </a>
                        </div>
                        <!-- Fit score -->
                        <div class="col-xs-6 col-sm-4 mtl mbs js-fitScore"></div>
                    </div>
                </div>
                <div class="hidden-xs col-sm-3 col-md-2 tar pvl">
                  <%= render 'compare_schools_compare_button' %>
                  <a class="btn btn-default zillow-button clearfix mts db js-homes-for-sale" target="_blank">
                      <div class="iconx16 i-16-blue-home fl mrs"></div>
                      <div class="fl"><%= t('.homes_for_sale') %></div>
                  </a>
                </div>
            </div>
        </div>

        <div class="col-xs-12 visible-xs clearfix phl pbm font-size-small">
            <div class="fl">
                <span class="prm vam js-parent-rating-stars"><%= draw_stars_16 1 %></span>
                <a class="font-size-small js-review-count"></a>
                <em class="js-no-reviews"><%= t('.no_community_reviews') %></em>
            </div>
            <div class="fr js-distance"></div>
        </div>

        <div class="pull-right visible-xs" style="width:100%">
          <%= render 'compare_schools_compare_button' %>
        </div>

    </div>
</div>
