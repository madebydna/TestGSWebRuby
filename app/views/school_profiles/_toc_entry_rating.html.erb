<% rating %>
<% title %>
<% anchor %>

<% if rating == 'NR' || rating == 'nr' %>
  <% rating = '' %>
<% end %>

<div class="clearfix toc-entry">
  <a href="#<%= anchor %>"
     class="js-gaClick"
     data-ga-click-category='Profile'
     data-ga-click-action='TOC Clicked'
     data-ga-click-label='<%= title %>'>
    <span><%= title %></span>
    <span class="icon-chevron-right"></span>
    <span class="gs-rating circle-rating--<%= rating %> circle-rating--xtra-small">
      <%= rating %><span class="denominator">/10</span>
    </span>
  </a>
  <% if badge.present? %>
    <a class="js-gaClick"
       data-ga-click-category='Profile'
       data-ga-click-action='TOC Clicked'
       data-ga-click-label="College_success"
       href="#College_success">
      <span class="csa-badge"><%= image_tag image_path('school_profiles/csa-badge-toc.png') %></span>
    </a>
  <% end %>
</div>