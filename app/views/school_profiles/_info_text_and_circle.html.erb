<% cta %>
<% content %>
<% element_type %>

<div class="info-text-and-circle">
  <a data-remodal-target="modal_info_box" data-content-type="info_box"
     data-ga-click-element-type='<%= element_type %>'
     data-content-html="<%= content %>" class="speech-bubble" href="javascript:void(0)">
    <% # Allow no whitespace between cta and info circle so we can control the margin %>
    <%= cta %><span class="info-circle icon-question"></span>
  </a>
</div>
