<div id="profile-sticky-container" class="js-sticky-right-rail">
  <div class="rr-nearby-school">
    <div class="content">
      <div class="icon">
        <%= image_tag (asset_path('icons/nearby schools_30x30.png')) %>
      </div>
      <div class="text">
        <%= t("school_profiles.We found 3 nearest high-performing schools") %><br />
        <a href="#NearbySchools"
           class="js-gaClick"
           data-ga-click-category='Profile'
           data-ga-click-action='Compare CTA Clicked'
           data-ga-click-label='Desktop mini module' ><%= t("school_profiles.View the schools") %></a>
      </div>
    </div>
  </div>
  <div class="rr-add-spacer-between-ads">
    <%= render 'shared/ad', slot: 'Profiles_First', size_mapping: 'box_or_tall',
             width: 300, height: 250, css_class: 'hidden-xs hidden-sm' %>
  </div>
  <%= render 'shared/ad', slot: 'Profiles_SecondSticky', size_mapping: 'box_or_tall',
             width: 300, height: 250, css_class: 'hidden-xs hidden-sm' %>
</div>