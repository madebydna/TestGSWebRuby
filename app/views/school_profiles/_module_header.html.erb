 <% circle_color_modifier %>
 <% circle_content  %>
 <% title %>
 <% subtitle ||= nil %>
 <% info_text = info_text || nil %>
 <% has_data %>
 <div class="module-header">
   <% unless circle_content.nil? || circle_content == 'NR' %>
       <div class="circle-rating--<%= circle_color_modifier %> circle-rating--medium">
         <%= circle_content %><span class="rating-circle-small">/10</span>
       </div>
   <% end %>
   <div class="title-container">
     <div>
       <span class="title">
         <%= title %>
       </span>
       <% if info_text %>
         <%= render 'info_circle', content: info_text %>
       <% end %>
     </div>
     <% unless subtitle.nil? %>
       <span><%= subtitle %></span>
     <% end %>
     <% unless has_data %>
       <%= render 'no_data' %>
     <% end %>
   </div>
 </div>