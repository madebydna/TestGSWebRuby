<% title = t('.title') %>
<a class="anchor-mobile-offset" name="Equity"></a>
<div class="equity-container rs-equity">
  <div class="row">
    <div class="col-xs-12 col-lg-3">
      <div class="rating-container__title">
        <%= title %>
      </div>
    </div>
    <div class="col-xs-12 col-lg-9">
      <%= react_component('Equity', test_scores:  equity.test_scores_by_ethnicity,
                          enrollment: equity.enrollment,
                          characteristics: equity.characteristics,
                          rating_low_income: equity.rating_low_income)  %>
    </div>
  </div>
</div>
