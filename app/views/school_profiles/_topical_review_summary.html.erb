<% reviews %>
<% title = t('.title') %>
<% topical_review_summary = reviews.summary.topical_review_summary %>

<% if topical_review_summary.present? %>
  <%= react_component(
    'TopicalReviewSummary', props: {
      title: title,
      topics: topical_review_summary.keys.sort,
      distributions_by_topic: reviews.summary.topical_review_distributions,
      summaries_by_topic: topical_review_summary
    }
  ) %>
<% end %>

