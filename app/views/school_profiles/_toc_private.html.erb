<% claimed_string = claimed.to_s %>

<div id="toc_private">
  <div class="toc-private-header">
    <% translation_type = 'school_profiles.show_profile.' + claimed_string + '_header' %>
    <ul>
      <li><%= I18n.t(translation_type + '.first_bullet') %></li>
      <li><%= I18n.t(translation_type + '.second_bullet_html') %></li>
    </ul>
    <ul>
      <li><%= I18n.t(translation_type + '.third_bullet') %></li>
      <li><%= I18n.t(translation_type + '.fourth_bullet') %></li>
    </ul>
  </div>
  <div class="toc-private-container-box">
    <div class="row">
      <% I18n.t('.toc', scope:'school_profiles.show_profile').each do |column_data| %>
        <div class="col-xs-12 col-sm-4">
          <div class="toc-private-section">
            <div class="toc-private-section-title">
              <a><%= column_data[:title].upcase %></a>
            </div>
            <div>
              <%= column_data[:description] %>
            </div>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>
