<% icon %>
<% title %>
<% anchor %>
<% flagged %>

<div class="clearfix toc-entry">
  <a href="#<%= anchor %>"
     class="js-gaClick"
     data-ga-click-category='Profile'
     data-ga-click-action='TOC Clicked'
     data-ga-click-label='<%= title %>'>
    <span><%= title %></span>
    <span class="icon-chevron-right"></span>
  </a>
  <% if flagged %>
    <a class="js-gaClick indicator-box red"
       data-ga-click-category='Profile'
       data-ga-click-action='TOC Clicked'
       data-ga-click-label="TOC - discipline/attendance flag"
       href="#<%= anchor %>">
      <span class="icon-flag"></span>
    </a>
  <% end %>
</div>
