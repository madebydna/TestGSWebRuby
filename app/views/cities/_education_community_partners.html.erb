<% unless @partners.nil? %>
  <% partners = show_tabs ? @partners[@tab] : @partners.values.flatten %>
  <% partners.each do |partner| %>
    <div class="row">
      <div class="community-partner-row phm">
        <hr>
        <%= image_tag(partner[:logo], alt: 'Partner logo', class: 'mbm') %>
        <div>
          <h3 class='mtn'><%= partner[:heading] %></h3>
          <p><%= partner[:description] %></p>
          <% partner[:links].each do |link| %>
            <% if link[:newwindow] == 'true' %>
              <%= link_to(link[:heading], link[:url], class: 'small', target: 'blank') %>
            <% else %>
              <%= link_to(link[:heading], link[:url], class: 'small') %>
            <% end %>
          <% end %>
        </div>
      </div>
    </div>
  <% end %>
<% else %>
  <h3>No Data Found - Key eduCommPage_partnerData</h3>
<% end %>
