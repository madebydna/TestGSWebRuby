<% if defined?(partners) && !partners.try(:[], :resources).nil? %>
  <div class="row mvl phm">
    <h2 class='mtn'><%= partners[:heading] %></h2>
    <h6 class='mvl'><%= partners[:subheading] %></h6>

      <div class="col-xs-12 mtl">
        <% partners[:resources].each do |resource| %>
          <div class="row clearfix mvm">
            <%= image_tag(resource[:imagePath], class: 'mrm mbm', align: 'left') %>
            <h4 class='mtn'><%= resource[:heading] %></h4>
            <%= resource[:content].html_safe %>
          </div>
        <% end %>
      </div>
  </div>
<% end %>
