    }

    .univeral-navigation .navigation {
      /*float:left;*/
      padding: 0px;
      width: 100%;
    }

    .search_icon {
      display: table-cell;
    }

    nav {
      margin: 0;
      /*clear:both;*/
    }

    .toggle + a {
      display: none;
    }

    .toggle {
      display: block;
      background-color: #FFFFFF;
      padding: 5px 20px;
      color: #000;
      font-size: 14px;
      line-height: 28px;
      text-decoration: none;
      border: none;
      width: 100%;
    }

    .toggle:hover {
      background-color: #fff;
    }

    [id^=drop]:checked + ul {
      display: block;
    }

    nav ul {
      width: 100%;
    }

    nav ul li {
      display: block;
      width: 100%;
      font: 14px "opensans-regular", Arial, "Helvetica Neue", Helvetica, sans-serif;
      position: relative;
    }

    nav ul ul .toggle,
    nav ul ul a {
      padding: 5px 40px;
      width: 100%;
    }

    nav ul ul ul a {
      background-color: #FFFFFF;
      padding: 0 80px;
    }

    nav ul ul ul a:hover {
      background-color: #fff;
    }

    nav ul li ul li .toggle,
    nav ul ul a {
      background-color: #FFFFFF;
      position: relative;
    }

    nav ul ul {
      float: none;
      top: 0px;
      border: none;
      color: #ffffff;
      position: relative;
      width: 100%;
      z-index: 1;
    }

    nav ul ul li:hover > ul,
    nav ul li:hover > ul {
      display: none;
    }

    nav ul ul li {
      display: block;
      width: 100%;
      position: relative;
    }

    nav ul ul ul li {
      /*position: static;*/
      position: relative;

    }

    nav ul ul ul {
      position: relative;
      top: 0px;
      left: 0px;
      width: 100%;
    }
  }

