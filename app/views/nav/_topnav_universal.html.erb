  .header {
    width: 100%;
    background-color: #FFFFFF;
  }

  /* menu */

  .header .menu {
    clear: both;
    max-height: 0;
    transition: max-height .2s ease-out;
  }

  /* menu icon */

  .header .menu-icon {
    cursor: pointer;
    /*display: inline-block;*/
    /*float: right;*/
    padding: 0px;
    vertical-align: middle;
    position: absolute;
    user-select: none;
    height: 30px;
    width: 30px;
    top: 20px;
  }

  .header .menu-icon .navicon {
    background: #333;
    display: block;
    height: 2px;
    position: absolute;
    transition: background .2s ease-out;
    width: 18px;
    top: 10px;
  }

  .header .menu-icon .navicon:before,
  .header .menu-icon .navicon:after {
    background: #333;
    content: '';
    display: block;
    height: 100%;
    position: absolute;
    transition: all .2s ease-out;
    width: 100%;
  }

  .header .menu-icon .navicon:before {
    top: 5px;
  }

  .header .menu-icon .navicon:after {
    top: -5px;
  }

  /* menu btn */

  .header .menu-btn {
    display: none;
  }

  .header .menu-btn:checked ~ .menu {
    max-height: 240px;
  }

  .header .menu-btn:checked ~ .menu-icon .navicon {
    background: transparent;
  }

  .header .menu-btn:checked ~ .menu-icon .navicon:before {
    transform: rotate(-45deg);
  }

  .header .menu-btn:checked ~ .menu-icon .navicon:after {
    transform: rotate(45deg);
  }

  .header .menu-btn:checked ~ .menu-icon:not(.steps) .navicon:before,
  .header .menu-btn:checked ~ .menu-icon:not(.steps) .navicon:after {
    top: 0;
  }

  .search_bar_size {
    width: 100%;
  }

  .search_icon {
    display: none;
  }

  nav {
    margin: 0;
    padding: 0;
    /*background-color: #FFFFFF;*/
  }

  nav:after {
    content: "";
    /*display: table;*/
    clear: both;
  }

  nav ul {

    padding: 0;
    margin: 0;
    list-style: none;
    position: relative;
    width: 450px;

  }

  nav ul li {
    margin: 0px;
    display: inline-block;
    float: left;
    /*background-color: #FFFFFF;*/
  }

  nav a {
    display: block;
    padding: 0px 20px;
    color: #000;
    font-size: 14px;
    line-height: 28px;
    text-decoration: none;
  }

  nav a:hover {
