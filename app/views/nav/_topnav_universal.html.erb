  }

  .search_bar_button {
    display: table-cell;
    vertical-align: middle;
  }

  /* Media Queries
  --------------------------------------------- */

  @media all and (max-width: 767px) {

    .univeral-navigation {
      display: block;
    }

    .univeral-navigation .tr {
      display: block;
    }

    .univeral-navigation .tc {
