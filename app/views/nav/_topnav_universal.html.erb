  /* menu icon */

  .un .header .menu-icon {
    cursor: pointer;
    /*display: inline-block;*/
    /*float: right;*/
    padding: 0px;
    vertical-align: middle;
    position: absolute;
    user-select: none;
    height: 20px;
    width: 20px;
    top: 12px;
  }

  .un .header .menu-icon .navicon {
    background: #333;
    display: block;
    height: 2px;
    position: absolute;
    transition: background .2s ease-out;
    width: 18px;
    top: 10px;
  }

  .un .header .menu-icon .navicon:before,
  .un .header .menu-icon .navicon:after {
    background: #333;
    content: '';
    display: block;
    height: 100%;
    position: absolute;
    transition: all .2s ease-out;
    width: 100%;
  }

  .un .header .menu-icon .navicon:before {
    top: 5px;
  }

  .un .header .menu-icon .navicon:after {
    top: -5px;
  }

  /* menu btn */

  .un .header .menu-btn {
    display: none;
  }

  .un .header .menu-btn:checked ~ .menu {
    max-height: 240px;
  }

  .un .header .menu-btn:checked ~ .menu-icon .navicon {
    background: transparent;
  }

  .un .header .menu-btn:checked ~ .menu-icon .navicon:before {
    transform: rotate(-45deg);
  }

  .un .header .menu-btn:checked ~ .menu-icon .navicon:after {
    transform: rotate(45deg);
  }

  .un .header .menu-btn:checked ~ .menu-icon:not(.steps) .navicon:before,
  .un .header .menu-btn:checked ~ .menu-icon:not(.steps) .navicon:after {
    top: 0;
  }

