    font-size: 8px;
  }

  li > a:only-child:after {
    content: '';
  }

  nav ul li ul li:hover {
    background: #FFFFFF;
  }

  nav ul ul {
    display: none;
    position: absolute;
    top: 25px;
    border: 1px solid #555566;
    background-color: #FFFFFF;
    width: 150px;
    z-index: 1;
    /*border-bottom: none;*/
  }

  nav ul ul a {
    padding: 5px 10px;
  }

  nav ul li:hover > ul {
    display: inherit;
  }

  nav ul ul li {
    width: 150px;
    float: none;
    display: list-item;
    position: relative;
    border-bottom: 1px solid #0000DD;
  }

  nav ul ul ul {
    position: absolute;
    /*background-color: #FFFFFF;*/
    top: -42px;
    left: 150px;
    width: 150px;
  }

  nav ul ul ul li:hover {
