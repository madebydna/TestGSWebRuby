<% state ||= nil %>

<%= render 'nav/sign_up_for_updates' %>

<div class="row invert-colors">
  <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 tac pam">
    <div class="limit-width-1200 open-sans">

      <%= render 'nav/about_gs_footer' %>

      <% unless state.blank? %>
        <%= render 'nav/popular_cities_in_state', state: state %>
      <% else %>
        <%= render 'nav/popular_cities_in_country' %>
      <% end %>

      <hr/>

      <%= render 'nav/gs_address_and_copyright' %>

    </div>
  </div>
</div>
