<% state %>
<%= render 'nav/sign_up_for_updates' %>

<div class="row invert-colors">
  <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 tac pam">
    <div class="limit-width-1200 open-sans">
      <%= render 'nav/about_gs_footer' %>

      <%= render 'nav/popular_cities_in_state', state: state %>

        <% state_name_long = state[:long].titleize %>
        <% if state[:short].downcase == 'dc' %>
            <% state_name_long = "Washington DC" %>
        <% end %>
      <span class='gray-dark'><%= t('.all_cities_in', state: state_name_long) %></span>

      <ul class='di'>
        <% ('A'..'Z').to_a.each do |letter| %>
          <li class='di'><%= link_to(letter, city_alphabet_path(gs_legacy_url_encode(state[:long].capitalize), state[:short].upcase, letter)) %></li>
        <% end %>
      </ul>

      <hr/>

      <%= render 'nav/gs_address_and_copyright' %>
    </div>
  </div>
</div>
<div class="gs_ad_slot visible-lg visible-md" id="adCustom_Welcome_Ad" data-ad-size="[1, 1]" style="width: 0px" data-dfp="Custom_Welcome_Ad"></div>
