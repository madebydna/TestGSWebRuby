{
  "extends": ["airbnb", "prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true
  },
  "plugins": ["prettier"],
  "settings": {
    "import/resolver": {
      "babel-module": {
        "alias": {
          "react_components": "./app/bundles/GSWeb/react_components",
          "util": "./app/bundles/GSWeb/util",
          "api_clients": "./app/bundles/GSWeb/api_clients"
        }
      }
    }
  }
}
